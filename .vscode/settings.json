{
  "latex-workshop.latex.outDir": "%DIR%",
  "files.exclude": {
    "**/*.aux": true,
    "**/*.log": true,
    "**/*.gz": true,
    "**/*.fls": true,
    "**/*.toc": true,
    "**/*.fdb_latexmk": true,
    "**/*.out": true
  },
  "editor.tabSize": 2,
  "code-runner.executorMap": {
    "scheme": "python3 $workspaceRoot/CS61A/scheme.py $workspaceRoot/CS61A/__experiments__/scheme/$fileName"
  }
}
